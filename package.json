{
	"name": "nobaan-interview",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "yarn run build && vite preview",
		"prettier:check": "prettier --check \"src/**/*.{ts,tsx}\"",
		"prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
		"eslint:check": "eslint \"src/**/*.{ts,tsx}\"",
		"eslint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
		"spell": "cspell \"src/**/*\"",
		"pre-commit": "yarn run prettier:check && yarn run eslint:check && yarn run spell",
		"cypress:open": "cypress open --e2e",
		"test": "concurrently -c \"auto\" \"cross-env hasDisabledPwa=true yarn run preview --strictPort --port 8080\" \"yarn run cypress:open\""
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.1",
		"bootstrap": "^5.2.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^3.1.4",
		"react-icons": "^4.7.1",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.6.1",
		"styled-components": "^5.3.6"
	},
	"devDependencies": {
		"@fullstacksjs/eslint-config": "^8.10.0",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"@vitejs/plugin-react": "^3.0.0",
		"babel-plugin-styled-components": "^2.0.7",
		"classnames": "^2.3.2",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"cspell": "^6.18.1",
		"cypress": "^12.3.0",
		"eslint": "^8.31.0",
		"husky": "^8.0.3",
		"prettier": "^2.8.1",
		"sass": "^1.57.1",
		"typescript": "^4.9.4",
		"values.js": "^2.1.1",
		"vite": "^4.0.0",
		"vite-plugin-pwa": "^0.14.1"
	}
}
